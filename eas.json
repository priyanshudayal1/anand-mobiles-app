{
  "cli": {
    "version": ">= 16.25.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "@google-services-json"
      },
      "prebuildCommand": "npm run restore:google-services"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "@google-services-json"
      },
      "prebuildCommand": "npm run restore:google-services"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "@google-services-json"
      },
      "prebuildCommand": "npm run restore:google-services"
    }
  },
  "submit": {
    "production": {}
  }
}
